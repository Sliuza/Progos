import java.util.*;
import java.util.Scanner;


public class CriadorTestes {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int range,tam = 0;
		Random g = new Random();
		Scanner l = new Scanner(System.in);
		tam = l.nextInt();
		range = l.nextInt();
		System.out.println(tam);
		for(int i =0;i<tam;i++){
			System.out.println(g.nextInt(5));
		}
		
		System.out.println(tam);
		l.close();
	}

}
